<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.rabbit.service.template.dao.DocumentTemplateMapper">
	<resultMap type="org.rabbit.entity.template.DocumentTemplate" id="resultMap">
	    <id property="id" column="id"/>
		<result property="remarks" column="remarks" />
		<result property="name" column="name" />
		<result property="documentId" column="document_id" />
		<result property="fileType" column="file_type" />
		<result property="templateVariable" column="template_variable" />
		<result property="description" column="description" />
		<result property="delFlag" column="del_flag" />
		<result column="create_by" jdbcType="VARCHAR" property="createBy" />
	    <result column="create_date" jdbcType="TIMESTAMP" property="createDate" />
	    <result column="update_by" jdbcType="VARCHAR" property="updateBy" />
	    <result column="update_date" jdbcType="TIMESTAMP" property="updateDate" />
	    <result column="remarks" jdbcType="VARCHAR" property="remarks" />
	</resultMap>

	<sql id="baseColumns">
		dt.id,
		dt.name,
		dt.document_id,
		dt.file_type,
		dt.template_variable,
		dt.description,
		dt.create_date,
		dt.update_date,
		dt.create_by,
		dt.update_by,
		dt.remarks,
		dt.del_flag
	</sql>

</mapper>